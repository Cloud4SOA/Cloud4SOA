<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

        <g:TabLayoutPanel barUnit="EM" barHeight="3" width="100%" height="23em" ui:field="tabLayout">
            <g:tab>
                <g:header>Application profile</g:header>

                <g:SimplePanel ui:field="applicationProfilePanel"/>

            </g:tab>


            <g:tab>
                <g:header>PaaS Offer</g:header>

                <g:SimplePanel ui:field="paaSOfferPanel"/>

            </g:tab>

            <g:tab>
                <g:header>Interaction style</g:header>
                <g:HorizontalPanel spacing="10" width="100%">
                    <g:ListBox width="180px" ui:field="interactionStyleList"/>
                </g:HorizontalPanel>
            </g:tab>
        </g:TabLayoutPanel>



</ui:UiBinder>
