      <owl:Class rdf:about="#Application_Physical"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Information_Relationship">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
