      <owl:Class rdf:about="#External_Integration"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Technology_Build_Architecture_Type">
    <rdfs:subClassOf rdf:resource="#Logical_Technology_Model"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
