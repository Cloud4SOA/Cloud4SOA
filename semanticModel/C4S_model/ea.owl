    </owl:unionOf>
  </owl:Class>
  <owl:Class rdf:about="#_TCA-RELATION">
    <rdfs:subClassOf rdf:resource="#_Technology_Relationship"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
