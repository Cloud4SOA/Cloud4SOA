    <rdfs:subClassOf rdf:resource="#EA_Relation"/>
  </owl:Class>
  <owl:Class rdf:ID="Information_Representation_Attribute">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Information_Logical"/>
