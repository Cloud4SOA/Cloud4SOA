      <owl:Class rdf:about="#Application_Logical"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Information_View_Attribute">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Information_Logical"/>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A particular attribute or item of information that is used or shared in the definition of Information Views</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Information_View_Attribute</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Static_Application_Provider_Architecture">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Static_Application_Provider_Architecture</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The architecture describing the sub components of an Application Provider - a STATIC view.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Logical_Application_Model"/>
  </owl:Class>
  <owl:Class rdf:ID="_ASU-TO-ASU-STATIC-RELATION">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Relation between Application Service Usages. Models the dependencies between Application Services in a static model.</rdfs:comment>
