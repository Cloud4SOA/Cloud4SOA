    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Class to represent Business Events that are raised by schedules or are timed events</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Business_Event"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#_Business_Relationship">
